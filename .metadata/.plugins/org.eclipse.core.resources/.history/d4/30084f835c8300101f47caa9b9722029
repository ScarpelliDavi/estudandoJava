package application;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Stream;


public class Program {

	public static void main(String[] args) {
		
		List<Integer> ls = Arrays.asList(1,2,5,6,7,8);
		
		Stream<Integer> st = ls.stream().map(p -> p*10);
		System.out.println(Arrays.toString(st.toArray()));
		
		Stream<String> st2 = Stream.of("Maria", "Davi", "Joao", "Alberto");
		System.out.println(Arrays.toString(st2.toArray()));
		
		Stream<Integer> st3 = Stream.iterate(0, x -> x + 2);
		System.out.println(Arrays.toString(st3.limit(10).toArray()));
		
		Stream<Long> stFibonacciStream = Stream.iterate(new Long[] {0L, 1L}, x -> new Long[] {x[1], x[0] + x[1]}).map(x -> x[0]);

	}

}
